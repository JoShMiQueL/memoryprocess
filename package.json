{
  "name": "@joshmiquel/memoryjs",
  "version": "3.5.8",
  "description": "Maintained fork of memoryjs with critical fixes. Includes /Zc:strictStrings compiler fixes and other improvements. Original package by Rob--",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm run build",
    "build": "node ./scripts/install.js",
    "build32": "node-gyp clean configure build --arch=ia32",
    "build64": "node-gyp clean configure build --arch=x64",
    "buildtest": "cd test && MSBuild.exe project.sln //p:Configuration=Release",
    "debug": "node ./scripts/debug.js",
    "debug32": "node-gyp configure rebuild --debug --arch=xia32",
    "debug64": "node-gyp configure rebuild --debug --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoShMiQueL/memoryjs.git"
  },
  "keywords": [
    "memory",
    "reading",
    "writing",
    "management",
    "addon",
    "maintained-fork",
    "windows-memory",
    "process-memory"
  ],
  "author": "JoShMiQueL",
  "contributors": [
    {
      "name": "Rob--",
      "url": "https://github.com/Rob--"
    }
  ],
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/JoShMiQueL/memoryjs/issues"
  },
  "homepage": "https://github.com/JoShMiQueL/memoryjs#readme",
  "dependencies": {
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "node-addon-api": "^3.2.1"
  }
}
